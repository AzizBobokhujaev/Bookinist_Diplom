@model List<Bookinist.Models.Entity.User>
<br />
<table class="table table-dark">
    <thead class="thead">
        <tr>
            <th>Id</th>
            <th>Логин</th>
            
            <th>Номер телефона</th>
            <th>Электронный адрес</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var account in Model)
        {
                <tr>
                    <td>@account.Id</td>
                    <td>@account.UserName</td>

                    <td>@account.PhoneNumber</td>
                    <td>@account.Email</td>

                    <td><a class="btn btn-info" href="~/Account/Edit?id=@account.Id">Изменить</a></td>
                </tr>
        }
    </tbody>
</table>